<?xml version="1.0" ?>

<bulletml type="vertical">

<action label="top">
	<repeat><times>3000000</times><action>
		<fire>
			<direction type="absolute">270</direction>
			<speed>1000</speed>
			<bullet/>
	  	</fire>
		<fire>
			<bulletRef label="topShot">
				<param>315</param>
			</bulletRef>
	  	</fire>
		<fire>
			<bulletRef label="topShot">
				<param>225</param>
			</bulletRef>
		</fire>
		<fire>
			<bulletRef label="topShot">
				<param>247</param>
			</bulletRef>
		</fire>
		<fire>
			<bulletRef label="topShot">
				<param>292</param>
			</bulletRef>
		</fire>
		<wait>4</wait>
	</action></repeat>
	<vanish />
</action>

<bullet label="topShot">
	<direction type="absolute">$1</direction>
	<speed>1000</speed>
	<action>
		<changeDirection>
			<direction type="absolute">-90</direction>
			<term>5</term>
		</changeDirection>
	</action>
</bullet>
</bulletml>

