<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">

<bulletml type="vertical"
          xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
	<repeat><times>200</times>
		<action>
		<repeat><times>16</times>
			<action>
				<fire>
				<speed>150</speed>
				<direction type="sequence">22.5</direction>
				<bullet/>
				</fire>
			</action>
		</repeat>
		<wait>100</wait>
		</action>
	</repeat>
</action>
</bulletml>
